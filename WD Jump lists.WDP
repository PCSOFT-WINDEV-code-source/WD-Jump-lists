  p_codes :
   -
     code : |1-
      
      global
      	MaJumpList est une JumpList
      	MaJumpList.IdentifiantApplication = "PCSOFT.JumpList." + VersionWinDev(versionFramework)
      
      local
      	
      si enmodetest() ALORS	
      	Avertissement("Pour profiter des fonctionnalités de cet exemple, il est préférable de lancer depuis l'exécutable et non en mode test")	
      FIN
      
      // La JumpList permet entre autres d'ajouter des tâches.
      // Ces tâches peuvent ré-exécuter votre application avec un ou plusieurs paramètres de ligne de commande.
      // Quand votre application est en exécution, pour éviter qu'une autre instance s'exécute, il est recommandé de vérifier les paramètres de lignes de commande et de les transmettre
      // à l'instance déjà en cours d'exécution.
      
      // On vérifie si il y a déjà une instance en cours d'exécution
      si exelance(exeinfo(exenom)) > 1 ALORS
      	
      	// On récupère le handle de la fenêtre "système" de l'application (ici WDJumpListWindow)
      	// qui recevra les messages qu'on lui enverra
      	nHandleInstanceExécutée est un entier systeme = SysFenHandle("WDJumpListWindow")
      	
      	// On commence par redonner le focus à l'application
      	SysFenActive(nHandleInstanceExécutée)
      	
      	// Récupère la ligne de commande
      	sLigneCommande est une chaine = lignecommande()
      		
      	// Ouvre une zone mémoire partagée
      	nIDZoneMémoire est un entier = fMemOuvre("LigneCommandeExterne", 1024)
      	
      	// Si on a réussi à ouvrir la zone
      	si nIDZoneMémoire > 0 alors
      		
      		// Inscrit la ligne de commande dans la zone
      		fEcrit(nIDZoneMémoire, sLigneCommande)
      		
      		// Envoie un message à l'instance en exécution pour la prévenir
      		postMessage(nHandleInstanceExécutée, "LigneCommandeExterne", taille(sLigneCommande), exedonnepid)
      	fin
      	
      	// Termine l'application
      	finprogramme(vrai)
      sinon
      	
      	// Ouvre une fenêtre cachée FEN_Système qui s'occupera de recevoir des informations d'autres instances
      	OuvreFille(FEN_Système)	
      	
      	// Traite la ligne de commande
      	TraiteLigneCommande()
      FIN
