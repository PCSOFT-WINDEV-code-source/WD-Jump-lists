   -
     name : CréeJumpList
     internal_properties : CAAAAAgAAAAMJZY8JlTN5RDufgoubezvYLaa+gjN7E9gLwBskacMhlomxe0dl+SV/m2sjbSVOX+kmOHxJh505SggmxfIxMl+8vkhaNLM8RW9GAg9NuECT1na7sFcxRzOg9KfaR/zltPzbwLkw+3S3RdC8x3M5yJPeVVJMa7SSY6B/m/E5+yim1HbZ4OZUPQKpNp/xEZx9S16FVSjF93OPotLtE4VFXiYX+3RN1bldJcEcKufa+2X/+x1dbsa9C+WJJl6lhntj3jD3gorqYlb1RUGFL8=
     procedure_id : 1958880269555361749
     type_code : 14
     code : |1-
      // Résumé : Crée la JumpList
      // Syntaxe :
      // CréeJumpList ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE CréeJumpList()
      
      	sExécutable est une chaine
      	sExécutable = (enmodetest() ? frepexe() + ["\"] + projetinfo(pinomprojet) + ".exe" sinon exeinfo(exenom))
      		
      	Séparateur est une jumplisttâche
      	Séparateur.EstSéparateur = vrai
      
      	// Ajoute la tâche d'ouverture dans l'explorateur
      	INDICE_TACHE_OUVRIREXP = JumpListTâcheAjoute(MaJumpList, JumpListCréeTâche("Ouvrir dans l'explorateur", sExécutable, "/open"))
      	JumpListTâcheAjoute(MaJumpList, Séparateur)
      	
      	// Ajoute les tâches de parcours
      	INDICE_TACHE_precedente = JumpListTâcheAjoute(MaJumpList, JumpListCréeTâche("Image précédente", sExécutable, "/previous"))
      	INDICE_TACHE_SUIVANTE   = JumpListTâcheAjoute(MaJumpList, JumpListCréeTâche("Image suivante", sExécutable, "/next"))
      	
      	// Icônes
      	MaJumpList.Tâches[INDICE_TACHE_PRECEDENTE].Raccourci.Icône = frepexe() + ["\"] + "precedent.ico"
      	MaJumpList.Tâches[INDICE_TACHE_SUIVANTE].Raccourci.Icône = frepexe() + ["\"] + "suivant.ico"
      	
      	// Actualise la JumpList
      	JumpListActualise(MaJumpList)
      
      	info("Pour visualiser la JumpList, cliquez avec le bouton droit de la souris sur l'icône de l'application dans la barre des tâches") 
     type : 458752
   -
     name : AfficheImage
     procedure_id : 1958882326845044041
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //AfficheImage ( [<nSens> est entier])
      //
      // Paramètres :
      // 	nSens (entier - valeur par défaut=0) : <indiquez ici le rôle de nSens>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE AfficheImage(local nSens est un entier = 0)
      
      	AnimationPrépare(IMG_AFFICHEE)
      
      	si 0 < gnindice < gtabImages..Occurrence ALORS
      		
      		// Ajoute l'image aux documents récents
      		SysDocRécentAjoute(gtabImages[gnindice])
      		
      		// Affiche l'image
      		IMG_AFFICHEE = gtabImages[gnIndice]		
      		LIB_NOMIMAGE = remplace(gtabImages[gnIndice], sysrep(srmesimages) + ["\"], "")
      		LIB_AUCUNEIMAGE..Visible = faux		
      	sinon
      		
      		LIB_NOMIMAGE = ""
      		IMG_AFFICHEE = ""
      		LIB_AUCUNEIMAGE..visible = vrai		
      	FIN
      	
      	si nSens = 1 alors
      		
      		// Fait défiler l'image depuis la gauche vers la droite
      		animationjoue(animDéfilementGauche)
      	sinon si nSens = -1 alors
      		
      		// Fait défiler l'image depuis la droite vers la gauche
      		AnimationJoue(animDéfilementDroite)
      	sinon
      		
      		// Fondu
      		AnimationJoue(animFondu)
      	fin
      	
     type : 458752
   -
     name : ImageSuivante
     procedure_id : 1958883112824111470
     type_code : 14
     code : |1-
      // Résumé : Passe à l'image suivante
      // Syntaxe :
      //ImageSuivante ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ImageSuivante()
      
      	// Indice suivant
      	gnIndice ++
      	
      	// Boucle si l'indice est supérieur aux nombres d'images
      	si gnindice > gtabImages..Occurrence alors gnindice = 1
      
      	// Affiche l'image
      	afficheimage(1)
     type : 458752
   -
     name : ImagePrécédente
     procedure_id : 1958883301802716174
     type_code : 14
     code : |1-
      // Résumé : Passe à l'image précédente
      // Syntaxe :
      //ImagePrécédente ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE ImagePrécédente()
      
      	// Indice précédent
      	gnIndice --
      	
      	// Boucle si l'indice est inférieur à 1
      	si gnindice < 1 alors gnindice = gtabImages..Occurrence
      
      	// Affiche l'image
      	AfficheImage(-1)
     type : 458752
