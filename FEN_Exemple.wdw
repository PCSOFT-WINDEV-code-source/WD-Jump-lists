     code : |1-
      
      global
      
      gtabImages 				est un tableau de chaines
      gnIndice   				est un entier
      INDICE_TACHE_SUIVANTE	est un entier
      INDICE_TACHE_PRECEDENTE	est un entier
      INDICE_TACHE_OUVRIREXP	est un entier
      
      local
      	
      // Récupère les images du répertoire "Mes Images"
      sListeImages est une chaine = flistefichier(sysrep(srmesimages) + ["\"] + "*.*", frRécursif)
      
      // Parcourt les fichiers
      pour toute chaine sImage de sListeImages separee par rc
      	
      	// Si c'est une image valide
      	si position(ffiltreimage(), "*" + fExtraitChemin(simage, fExtension)) > 0 ALORS
      		
      		// Conserve le chemin
      		ajoute(gtabImages, simage)
      	FIN
      FIN
      
      // Première image
      gnIndice = 1
      
      // Affiche l'image
      AfficheImage()
